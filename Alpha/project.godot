; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Resource",
"class": "Registros",
"language": "GDScript",
"path": "res://Codigos/Eternos/Dados.gd"
} ]
_global_script_class_icons={
"Registros": ""
}

[application]

config/name="Jogo"
run/main_scene="res://Cenas/Telas/TelaInicial.tscn"
boot_splash/image="res://Elementos/Visuais/Fundo/Tela.png"
boot_splash/bg_color=Color( 0, 0.392157, 0, 1 )
config/icon="res://Elementos/Visuais/Bandeiras/Brasil.png"
config/macos_native_icon="res://Elementos/Visuais/Bandeiras/Brasil.png"
config/windows_native_icon="res://Elementos/Visuais/Bandeiras/Brasil.png"
config/versao="0.1"

[autoload]

Carregar="*res://Cenas/Objetos/BotaoConfiguracoes.tscn"

[debug]

settings/crash_handler/message="O programador foi incapaz de eveitar esse erro. Pedimos desculpas e encaminharemos o programador para a cadeia mais proxima. "

[display]

window/size/width=1280
window/size/height=720
window/size/test_width=1280
window/size/test_height=720
window/dpi/allow_hidpi=true
window/handheld/orientation="sensor_landscape"
window/stretch/mode="2d"
window/stretch/aspect="expand"

[importer_defaults]

wav={
"compress/mode": 1,
"edit/loop": false,
"edit/normalize": false,
"edit/trim": false,
"force/8_bit": false,
"force/max_rate": false,
"force/max_rate_hz": 44100,
"force/mono": false
}
mp3={
"loop": false,
"loop_offset": 0
}

[input_devices]

pointing/emulate_touch_from_mouse=true

[locale]

fallback="pt_BR"
translations=PoolStringArray( "res://Elementos/Linguisticos/language.pt_BR.translation", "res://Elementos/Linguisticos/language.es_ES.translation", "res://Elementos/Linguisticos/language.en_GB.translation" )
locale_filter=[ 1, [ "en_GB", "es_ES", "pt_BR" ] ]

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
